<!-- Spotbugs filter for excluding reports that should not be considered errors.
See https://spotbugs.readthedocs.io/en/latest/filter.html
-->

<FindBugsFilter>

  <Match>
    <Bug pattern="EI_EXPOSE_REP"/>
  </Match>

  <Match>
    <Bug pattern="EI_EXPOSE_REP2"/>
  </Match>

  <Match>
    <Bug pattern="CT_CONSTRUCTOR_THROW"/>
  </Match>

  <Match>
    <Package name="ca.gc.aafc.collection.api.mapper" />
    <Class name="~.*MapperImpl$"/>
  </Match>

  <Match>
    <Class name="ca.gc.aafc.collection.api.service.StorageUnitService" />
    <Method name="handleOptionalFields" />
    <Bug pattern="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT" />
  </Match>
  
</FindBugsFilter>
