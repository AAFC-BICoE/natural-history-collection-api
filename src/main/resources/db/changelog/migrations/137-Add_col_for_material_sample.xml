                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://local.xsd/dbchangelog-4.4.xsd"
                   objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="137-Add_col_for_material_sample" context="schema-change" author="Brandon Andre">

    <!-- Add the pos column to material sample to keep track of the position in a list. -->
