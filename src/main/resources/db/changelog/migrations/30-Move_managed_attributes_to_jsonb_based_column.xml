<?xml version="1.1" encoding="UTF-8" standalone="no" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog ../xsd/dbchangelog-4.4.xsd"
                   objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="30-Move_managed_attributes_to_jsonb_based_column" context="schema-change" author="poffm">
        <addColumn tableName="collecting_event">
            <column name="managed_attribute_values" type="jsonb" defaultValue="{}" />
        </addColumn>
        <dropTable tableName="collecting_event_managed_attribute" />
    </changeSet>

</databaseChangeLog>
