<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd" context="schema-change">

	<changeSet context="schema-change" id="14-Add_collecting_event_to_georeference_assertion_relationship_table" author="ganx">
		<createTable tableName="collecting_event_georeference_assertion">
			<column autoIncrement="true" name="id" type="SERIAL">
				<constraints primaryKey="true" primaryKeyName="pk_collecting_event_georeference_assertion_id" />
			</column>
			<column name="collecting_event_id" type="integer">
				<constraints foreignKeyName="fk_collecting_event_georeference_assertion_to_collecting_event_id" references="collecting_event(id)" />
			</column>
			<column name="georeference_assertion_id" type="integer">
				<constraints foreignKeyName="fk_collecting_event_georeference_assertion_to_georeference_assertion_id" references="georeference_assertion(id)" />
			</column>
		</createTable>
	</changeSet>

</databaseChangeLog>
